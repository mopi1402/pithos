{
  "name": "pithos",
  "version": "0.0.1",
  "description": "Advanced JavaScript/TypeScript superset providing performance, gestures, animations, and DOM utilities",
  "exports": {
    "./animations/*": "./src/animations/*.ts",
    "./data/*": "./src/data/*.ts",
    "./data/parsers/*": "./src/data/parsers/*.ts",
    "./data/storage/*": "./src/data/storage/*.ts",
    "./dom/*": "./src/dom/*.ts",
    "./dom/browser/*": "./src/dom/browser/*.ts",
    "./gestures/*": "./src/gestures/*.ts",
    "./math/*": "./src/math/*.ts",
    "./performance/*": "./src/performance/*.ts",
    "./types/*": "./src/types/*.ts",
    "./types/animations/*": "./src/types/animations/*.ts",
    "./types/dom/*": "./src/types/dom/*.ts",
    "./types/math/*": "./src/types/math/*.ts"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "generate-exports": "ts-node -P tsconfig.scripts.json scripts/generate-exports.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run generate-exports"
  },
  "keywords": [
    "typescript",
    "javascript",
    "superset",
    "performance",
    "gestures",
    "animations",
    "dom",
    "utilities",
    "ecosystem"
  ],
  "author": "Pierre Moati <mopi1402>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mopi1402/pithos.git"
  },
  "bugs": {
    "url": "https://github.com/mopi1402/pithos/issues"
  },
  "homepage": "https://github.com/mopi1402/pithos#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^8.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-terser": "^7.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^4.9.0"
  },
  "peerDependencies": {
    "typescript": ">=4.9.0"
  }
}

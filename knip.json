        "src/kanon/v2/core/*.ts",
        "src/kanon/v2/schemas/coerce/*.ts",
        "src/kanon/v2/schemas/composites/*.ts",
        "src/kanon/v2/schemas/constraints/*.ts",
        "src/kanon/v2/schemas/primitives/*.ts",
        "src/kanon/v2/schemas/wrappers/*.ts",
        "src/kanon/v2/types/*.ts",
        "src/kanon/v2/utils/*.ts",

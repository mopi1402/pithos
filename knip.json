        "src/taphos/math/*.ts",
        "src/taphos/object/*.ts",
        "src/taphos/seq/*.ts",
        "src/taphos/string/*.ts",
        "src/taphos/util/*.ts",
        "src/zygos/*.ts",
        "src/zygos/result/*.ts"
      ],
      "project": [
        "src/**/*.ts",
        "!src/**/*.test.ts"
      ]
    }
  },
  "ignoreWorkspaces": [
    "packages/kanon/*",
    "packages/main/*",
    "packages/arkhe",
    "packages/tmp"
  ],
  "ignore": [
    "vendor/**",
    ".stryker-tmp/**",
    "scripts/**"
  ],
  "ignoreDependencies": [
    "@sinclair/typebox",
    "@typescript-eslint/eslint-plugin",
    "@typescript-eslint/parser",
    "ajv-formats",
    "c8",
    "esbuild",
    "fast-check",
    "fastest-validator",
    "neverthrow",
    "superstruct",
    "ts-node",
    "valibot",
    "vite",
    "zod",
    "zod3"
  ],
  "ignoreExportsUsedInFile": true,
  "rules": {
    "duplicates": "off"
  }

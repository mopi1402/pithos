  "workspaces": {
    "packages/pithos": {
      "entry": [
        "src/arkhe/array/*.ts",
        "src/arkhe/async/*.ts",
        "src/arkhe/function/*.ts",
        "src/arkhe/is/guard/*.ts",
        "src/arkhe/is/predicate/*.ts",
        "src/arkhe/number/*.ts",
        "src/arkhe/number/parsers/*.ts",
        "src/arkhe/object/*.ts",
        "src/arkhe/string/*.ts",
        "src/arkhe/string/parsers/*.ts",
        "src/arkhe/test/*.ts",
        "src/arkhe/types/animations/*.ts",
        "src/arkhe/types/common/*.ts",
        "src/arkhe/types/dom/*.ts",
        "src/arkhe/types/gestures/*.ts",
        "src/arkhe/types/math/*.ts",
        "src/arkhe/types/performance/*.ts",
        "src/arkhe/types/utilities/*.ts",
        "src/arkhe/util/*.ts",

        "src/kanon/v3/jit/*.ts",
        "src/kanon/v3/jit/utils/*.ts",
        "src/kanon/v3/schemas/coerce/*.ts",
        "src/kanon/v3/schemas/composites/*.ts",
        "src/kanon/v3/schemas/constraints/*.ts",
        "src/kanon/v3/schemas/constraints/refine/*.ts",
        "src/kanon/v3/schemas/operators/*.ts",

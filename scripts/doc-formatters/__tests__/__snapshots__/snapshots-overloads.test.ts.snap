  desktop={\`flowchart LR
    A["5"] --> B["√ó 2"] --> C["10"] --> D["+ 1"] --> E["11"]\`}
  mobile={\`flowchart TB
    A["5"] --> B["√ó 2"] --> C["10"] --> D["+ 1"] --> E["11"]\`}
/>


{
  "$schema": "./function-weights.schema.json",
  "description": "Performance weight categories for utility functions. Higher weight = more important to be fast.",
  "categories": {
    "CRITICAL": {
      "weight": 5,
      "description": "Hot path functions called in tight loops (map, filter, reduce, transformations)",
      "color": "#dc2626"
    },
    "HIGH": {
      "weight": 3,
      "description": "Frequently used utilities, but not in tight loops (get, set, pick, omit)",
      "color": "#f59e0b"
    },
    "MEDIUM": {
      "weight": 1,
      "description": "Occasional utilities (type guards, string formatting)",
      "color": "#3b82f6"
    },
    "LOW": {
      "weight": 0.5,
      "description": "Setup-only functions where runtime perf is irrelevant (debounce, throttle, once)",
      "color": "#6b7280"
    }
  },
